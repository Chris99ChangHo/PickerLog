{
  "expo": {
    "name": "PickerLog",
    "slug": "PickerLog",
    "scheme": "pickerlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PickerLog-Icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/PickerLog-Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#50D94D"       
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/PickerLog-Icon.png",
        "backgroundColor": "#50D94D"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
